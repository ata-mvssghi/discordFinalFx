<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="110.0" prefWidth="535.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.MusicPlayerController">
   <children>
      <Label fx:id="songLabel" alignment="CENTER" layoutY="1.0" prefHeight="55.0" prefWidth="535.0" text="MP3 Player" textFill="#1d9c69">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
      <ProgressBar fx:id="songProgressBar" layoutX="-1.0" layoutY="62.0" prefHeight="18.0" prefWidth="535.0" progress="0.0" />
      <Button fx:id="playButton" layoutY="80.0" mnemonicParsing="false" onAction="#playMedia" text="play" textFill="#21a941" />
      <Button fx:id="resetButton" layoutX="86.0" layoutY="80.0" mnemonicParsing="false" onAction="#resetMedia" text="reset" textFill="#bedf07" />
      <Button fx:id="pauseButton" layoutX="38.0" layoutY="80.0" mnemonicParsing="false" onAction="#pauseMedia" text="pause" textFill="#e71b1b" />
      <Button fx:id="previousButton" layoutX="128.0" layoutY="80.0" mnemonicParsing="false" onAction="#previousMedia" text="previous" textFill="#720dbf" />
      <Button fx:id="nextButton" layoutX="190.0" layoutY="80.0" mnemonicParsing="false" onAction="#nextMedia" text="next" textFill="#2325de" />
   </children>
</AnchorPane>
