                  <Insets left="510.0" top="5.0" />
               </HBox.margin>
            </ImageView>
         </children>
      </HBox>
      <Button layoutX="700.0" layoutY="459.0" mnemonicParsing="false" onMouseClicked="#send" style="-fx-background-color: #0be30f;" text="send" textFill="#f4efef" />
